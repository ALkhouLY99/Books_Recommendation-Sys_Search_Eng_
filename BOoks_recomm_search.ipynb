   "source": [
    "def parse_define(line):\n",
    "    data = json.loads(line)\n",
    "    return{\n",
    "        \"book_id\":data['book_id'],\n",
    "        \"title\":data['title'],\n",
    "        \"ratings\":data['ratings_count'],\n",
    "        \"url\":data['url'],\n",
    "        \"cover_image\":data['image_url']\n",
    "    }"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "filterd books count:531843\n"
     ]
    }
   ],
   "source": [
    "book_info =[]\n",
    "with gzip.open(\"goodreads_books.json.gz\") as f:\n",
    "    for line in f:\n",
    "        if not line:\n",
    "            continue\n",
    "        \n",
    "        try:\n",
    "            fileds = parse_define(line)\n",
    "            ratings = int(fileds.get('ratings',0))\n",
    "            if ratings > 90:\n",
    "                book_info.append(fileds)\n",
    "        except ValueError:\n",
    "            continue\n",
    "print(f\"filterd books count:{len(book_info)}\")"
   ]
  },
